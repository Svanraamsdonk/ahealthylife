<% include header %>

<h1>A healthy life </h1>




  <% var teller = 1; %>
  <% nieuws.forEach(function(item){ %>
    <section>
        <h2 class="titel">
          <a href=/items/<%- item.id %>><%- item.titel %></a>
        </h2>
        <img src="/img/<%- item.afbeeldings %>" alt="<%- item.titel %>">
        <p><%- item.datum %></p>

        <p>
          <%- item.samenvatting %>
        </p>
    </section>
    <% teller++;}); %>


<% include footer %>
